{
	"name":"LiveWeb",
	"description":"Tracking changes in web content across the internet",
	"version":"0.1.0",
	"manifest_version": 2,
	"icons": { },
	"background":{

		"scripts": ["./background.js"]
	},
	"options_page": "./options.html",
	"browser_action": {

		"default_popup":"popup.html"
	},
	"permissions": [
						"tabs",
						"<all_urls>"
					]

}